{
    "name": "squarenetmedia/secure-login",
    "description": "Secure Login and Registration Package",
    "keywords": [
        "squarenetmedia",
        "secure-login"
    ],
    "homepage": "https://github.com/squarenetmedia/secure-login",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "SquareNetMedia",
            "email": "info@squarenetmedia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Squarenetmedia\\SecureLogin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Squarenetmedia\\SecureLogin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Squarenetmedia\\SecureLogin\\SecureLoginServiceProvider"
            ],
            "aliases": {
                "SecureLogin": "Squarenetmedia\\SecureLogin\\SecureLoginFacade"
            }
        }
    }
}
